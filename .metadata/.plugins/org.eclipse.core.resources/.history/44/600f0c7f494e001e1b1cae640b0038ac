package com.springboot.login.impl;

import org.springframework.beans.factory.annotation.Autowired;

import com.springboot.login.entity.Signup;
import com.springboot.login.repo.SignupRepo;
import com.springboot.login.service.SignupService;

public class Signupimpl implements SignupService {

	@Autowired
	private SignupRepo signupRepo;
	
	@Override
	public Signup createUser(Signup signup) {
		// TODO Auto-generated method stub
		return signupRepo.save(signup);
	}

}
